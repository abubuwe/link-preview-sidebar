{
	"scripts": {
		"prettier": "prettier --check '**/*.{yml,ts,json}'",
		"typecheck": "tsc -p .",
		"eslint": "eslint 'src/**/*.ts'",
		"eslint:fix": "eslint 'src/**/*.ts' --fix",
		"start": "rollup -c -w",
		"build": "rollup -c",
		"release": "NODE_ENV=production rollup -c",
		"semantic-release": "semantic-release"
	},
	"webExt": {
		"sourceDir": "./dist/"
	},
	"browserslist": [
		"last 3 Chrome versions",
		"last 3 Firefox versions"
	],
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"dependencies": {
		"@sentry/browser": "^6.12.0",
		"@sentry/tracing": "^6.12.0",
		"dotenv": "^10.0.0",
		"jsx-dom": "^6.4.23"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@rollup/plugin-alias": "^3.1.1",
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^11.0.1",
		"@rollup/plugin-replace": "^3.0.0",
		"@rollup/plugin-typescript": "^8.1.0",
		"@semantic-release/github": "^7.2.0",
		"@sourcegraph/eslint-config": "^0.20.18",
		"@sourcegraph/prettierrc": "^3.0.3",
		"@types/chrome": "0.0.157",
		"eslint": "^7.15.0",
		"husky": "^4.3.0",
		"prettier": "^2.2.1",
		"rollup": "^2.56.3",
		"rollup-plugin-chrome-extension": "^3.6.1",
		"rollup-plugin-cpy": "^2.0.1",
		"rollup-plugin-empty-dir": "^1.0.5",
		"rollup-plugin-zip": "^1.0.1",
		"semantic-release": "^17.3.0",
		"semantic-release-chrome": "^1.1.3",
		"semantic-release-firefox": "^2.0.3",
		"typescript": "^4.1.2"
	}
}
